{
  "version": 2,
  "builds": [
    {
      "rewrites": [
        {
          "source": "/(.*)",
          "destination": "/"
        }
      ]
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/(.*)",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/img/fotos/(.*)",
      "dest": "/public/img/fotos/$1"
    },
    {
      "src": "/img/(.*)",
      "dest": "/public/img/$1"
    },
    {
      "src": "/$",
      "dest": "/frontend/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}